                                                                                                                                                                                                                                                                                                    <p class="text-center">
                                                                                                                                                                                                                                                                                                        <strong>INSTRUCTIONS</strong>
                                                                                                                                                                                                                                                                                                    </p>

                                                                                                                                                                                                                                                                                                    <p>
                                                                                                                                                                                                                                                                                                        &nbsp;</p>
                                                                                                                                                                                                                                                                                                    <ol class="text-justify">
                                                                                                                                                                                                                                                                                                        <li>
                                                                                                                                                                                                                                                                                                            Enter
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            proper
                                                                                                                                                                                                                                                                                                            institution
                                                                                                                                                                                                                                                                                                            name.
                                                                                                                                                                                                                                                                                                            This
                                                                                                                                                                                                                                                                                                            is
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            name
                                                                                                                                                                                                                                                                                                            that
                                                                                                                                                                                                                                                                                                            will
                                                                                                                                                                                                                                                                                                            be
                                                                                                                                                                                                                                                                                                            used
                                                                                                                                                                                                                                                                                                            in
                                                                                                                                                                                                                                                                                                            lists
                                                                                                                                                                                                                                                                                                            of
                                                                                                                                                                                                                                                                                                            accredited
                                                                                                                                                                                                                                                                                                            programs
                                                                                                                                                                                                                                                                                                            distributed
                                                                                                                                                                                                                                                                                                            to
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            public.
                                                                                                                                                                                                                                                                                                        </li>
                                                                                                                                                                                                                                                                                                        <li>
                                                                                                                                                                                                                                                                                                            List
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            exact
                                                                                                                                                                                                                                                                                                            program
                                                                                                                                                                                                                                                                                                            title
                                                                                                                                                                                                                                                                                                            (not
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            department
                                                                                                                                                                                                                                                                                                            name).
                                                                                                                                                                                                                                                                                                            This
                                                                                                                                                                                                                                                                                                            should
                                                                                                                                                                                                                                                                                                            be
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            title
                                                                                                                                                                                                                                                                                                            shown
                                                                                                                                                                                                                                                                                                            consistently
                                                                                                                                                                                                                                                                                                            on
                                                                                                                                                                                                                                                                                                            student
                                                                                                                                                                                                                                                                                                            transcripts
                                                                                                                                                                                                                                                                                                            and
                                                                                                                                                                                                                                                                                                            in
                                                                                                                                                                                                                                                                                                            your
                                                                                                                                                                                                                                                                                                            institutions
                                                                                                                                                                                                                                                                                                            catalog.
                                                                                                                                                                                                                                                                                                        </li>
                                                                                                                                                                                                                                                                                                        <li>
                                                                                                                                                                                                                                                                                                            Codes
                                                                                                                                                                                                                                                                                                            for
                                                                                                                                                                                                                                                                                                            evaluation
                                                                                                                                                                                                                                                                                                            types
                                                                                                                                                                                                                                                                                                            are:
                                                                                                                                                                                                                                                                                                            DA=
                                                                                                                                                                                                                                                                                                            Desk
                                                                                                                                                                                                                                                                                                            Audit
                                                                                                                                                                                                                                                                                                            or
                                                                                                                                                                                                                                                                                                            FA=Field
                                                                                                                                                                                                                                                                                                            Audit
                                                                                                                                                                                                                                                                                                            or
                                                                                                                                                                                                                                                                                                            CA=Comprehensive
                                                                                                                                                                                                                                                                                                            Audit
                                                                                                                                                                                                                                                                                                            (Both
                                                                                                                                                                                                                                                                                                            DA
                                                                                                                                                                                                                                                                                                            and
                                                                                                                                                                                                                                                                                                            FA).
                                                                                                                                                                                                                                                                                                        </li>
                                                                                                                                                                                                                                                                                                        <li>
                                                                                                                                                                                                                                                                                                            List
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            abbreviation
                                                                                                                                                                                                                                                                                                            of
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            degree
                                                                                                                                                                                                                                                                                                            granted
                                                                                                                                                                                                                                                                                                            upon
                                                                                                                                                                                                                                                                                                            successful
                                                                                                                                                                                                                                                                                                            completion
                                                                                                                                                                                                                                                                                                            of
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            program,
                                                                                                                                                                                                                                                                                                            e.g.,
                                                                                                                                                                                                                                                                                                            BSCS,
                                                                                                                                                                                                                                                                                                            BSSE,
                                                                                                                                                                                                                                                                                                            BSIT,
                                                                                                                                                                                                                                                                                                            etc.
                                                                                                                                                                                                                                                                                                        </li>
                                                                                                                                                                                                                                                                                                        <li>
                                                                                                                                                                                                                                                                                                            List
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            number
                                                                                                                                                                                                                                                                                                            of
                                                                                                                                                                                                                                                                                                            years
                                                                                                                                                                                                                                                                                                            normally
                                                                                                                                                                                                                                                                                                            required
                                                                                                                                                                                                                                                                                                            for
                                                                                                                                                                                                                                                                                                            a
                                                                                                                                                                                                                                                                                                            full-time
                                                                                                                                                                                                                                                                                                            student
                                                                                                                                                                                                                                                                                                            to
                                                                                                                                                                                                                                                                                                            complete
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            program.
                                                                                                                                                                                                                                                                                                        </li>
                                                                                                                                                                                                                                                                                                        <li>
                                                                                                                                                                                                                                                                                                            Enter
                                                                                                                                                                                                                                                                                                            an
                                                                                                                                                                                                                                                                                                            (X)
                                                                                                                                                                                                                                                                                                            if
                                                                                                                                                                                                                                                                                                            this
                                                                                                                                                                                                                                                                                                            will
                                                                                                                                                                                                                                                                                                            be
                                                                                                                                                                                                                                                                                                            an
                                                                                                                                                                                                                                                                                                            evaluation
                                                                                                                                                                                                                                                                                                            of
                                                                                                                                                                                                                                                                                                            a
                                                                                                                                                                                                                                                                                                            currently
                                                                                                                                                                                                                                                                                                            unaccredited
                                                                                                                                                                                                                                                                                                            program.
                                                                                                                                                                                                                                                                                                            Leave
                                                                                                                                                                                                                                                                                                            this
                                                                                                                                                                                                                                                                                                            entry
                                                                                                                                                                                                                                                                                                            blank
                                                                                                                                                                                                                                                                                                            if
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            program
                                                                                                                                                                                                                                                                                                            is
                                                                                                                                                                                                                                                                                                            currently
                                                                                                                                                                                                                                                                                                            accredited.
                                                                                                                                                                                                                                                                                                        </li>
                                                                                                                                                                                                                                                                                                        <li>
                                                                                                                                                                                                                                                                                                            The
                                                                                                                                                                                                                                                                                                            Chief
                                                                                                                                                                                                                                                                                                            Administrative
                                                                                                                                                                                                                                                                                                            Officer
                                                                                                                                                                                                                                                                                                            must
                                                                                                                                                                                                                                                                                                            sign
                                                                                                                                                                                                                                                                                                            this
                                                                                                                                                                                                                                                                                                            form.
                                                                                                                                                                                                                                                                                                            This
                                                                                                                                                                                                                                                                                                            is
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            individual
                                                                                                                                                                                                                                                                                                            responsible
                                                                                                                                                                                                                                                                                                            for
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            institution
                                                                                                                                                                                                                                                                                                            submitting
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            Request
                                                                                                                                                                                                                                                                                                            for
                                                                                                                                                                                                                                                                                                            Evaluation.
                                                                                                                                                                                                                                                                                                            This
                                                                                                                                                                                                                                                                                                            individual
                                                                                                                                                                                                                                                                                                            typically
                                                                                                                                                                                                                                                                                                            carries
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            title
                                                                                                                                                                                                                                                                                                            of
                                                                                                                                                                                                                                                                                                            vice
                                                                                                                                                                                                                                                                                                            chancellor
                                                                                                                                                                                                                                                                                                            or
                                                                                                                                                                                                                                                                                                            rector.
                                                                                                                                                                                                                                                                                                        </li>
                                                                                                                                                                                                                                                                                                        <li>
                                                                                                                                                                                                                                                                                                            Enter
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            web
                                                                                                                                                                                                                                                                                                            site
                                                                                                                                                                                                                                                                                                            URL
                                                                                                                                                                                                                                                                                                            that
                                                                                                                                                                                                                                                                                                            should
                                                                                                                                                                                                                                                                                                            be
                                                                                                                                                                                                                                                                                                            shown
                                                                                                                                                                                                                                                                                                            for
                                                                                                                                                                                                                                                                                                            your
                                                                                                                                                                                                                                                                                                            institution
                                                                                                                                                                                                                                                                                                            in
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            list
                                                                                                                                                                                                                                                                                                            of
                                                                                                                                                                                                                                                                                                            accredited
                                                                                                                                                                                                                                                                                                            programs
                                                                                                                                                                                                                                                                                                            on
                                                                                                                                                                                                                                                                                                            the
                                                                                                                                                                                                                                                                                                            NCEAC
                                                                                                                                                                                                                                                                                                            web
                                                                                                                                                                                                                                                                                                            site
